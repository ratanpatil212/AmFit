<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".lbp_other">

    <TextView
        android:id="@+id/textView28"
        android:layout_width="342dp"
        android:layout_height="332dp"
        android:background="@color/white"
        android:text="Boost Your B12 Intake : Foods that are rich in vitamin B12 include eggs, chicken, fish like salmon and tuna, and low-fat dairy products."
        android:textColor="#AC9D1B"
        android:textSize="34dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.192" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="339dp"
        android:layout_height="317dp"
        android:background="@color/white"
        android:text="Eat Salty Foods : Good sources of salt include olives, cottage cheese, and canned soup or tuna."
        android:textColor="#AA9C1F"
        android:textSize="34dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.513"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.961" />

</androidx.constraintlayout.widget.ConstraintLayout>